{
  "name": "recoil-sync-next",
  "version": "0.0.0",
  "description": "recoil-sync stores for Next.js",
  "keywords": [
    "react",
    "recoil",
    "recoil-sync",
    "next",
    "nextjs",
    "next.js"
  ],
  "main": "dist/cjs/index.js",
  "repository": "https://github.com/recruit-tech/recoil-sync-next.git",
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "clean": "rm -Rf dist coverage",
    "clean-build": "run-s clean build",
    "build": "run-s -c build:*",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit -p tsconfig.json",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "eslint src --fix",
    "lint-fix:prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "test": "jest",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run lint:eslint && yarn run test"
  },
  "peerDependencies": {
    "@recoiljs/refine": "^0.1.0",
    "next": "^12.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recoil": "^0.7.4",
    "recoil-sync": "^0.1.0",
    "transit-js": "^0.8.874"
  },
  "devDependencies": {
    "@jest/types": "28.1.1",
    "@recoiljs/refine": "0.1.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/transit-js": "0.8.0",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "jest-environment-jsdom": "28.1.2",
    "next": "12.2.2",
    "next-router-mock": "0.7.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.4",
    "recoil-sync": "0.1.0",
    "simple-git-hooks": "2.8.0",
    "transit-js": "0.8.874",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "files": [
    "src/",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx",
    "dist/",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.d.ts.map",
    "!examples/"
  ]
}
